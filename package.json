{
  "name": "backend-coding-test",
  "version": "1.1.0",
  "description": "A coding assessment for Xendit backend interviews",
  "main": "dist/index.js",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "test": "cross-env NODE_ENV=test mocha -p -r reflect-metadata -r ts-node/register src/**/*.test.ts -b",
    "test:load": "node scripts/load-test.js",
    "coverage": "nyc yarn test",
    "artillery": "artillery run src/tests/load/test.yml",
    "api": "node dist/index.js",
    "start": "cross-env NODE_ENV=production run-s clean compile api",
    "clean": "rimraf dist",
    "compile": "tsc",
    "compile:watch": "tsc --watch",
    "watch": "nodemon -q dist/index.js",
    "dev": "cross-env NODE_ENV=development run-p clean compile:watch watch",
    "lint": "eslint .",
    "pretty": "prettier . --write",
    "format": "run-s lint pretty",
    "prepare": "husky"
  },
  "keywords": [
    "Xendit"
  ],
  "author": "Bo",
  "license": "ISC",
  "dependencies": {
    "@hapi/boom": "^10.0.0",
    "cross-env": "^10.0.0",
    "express": "^5.0.0",
    "inversify": "^7.0.0",
    "joi": "^18.0.0",
    "reflect-metadata": "^0.2.0",
    "sequelize": "^6.23.2",
    "sequelize-typescript": "^2.1.3",
    "sqlite3": "^5.0.2",
    "swagger-ui-express": "^5.0.0",
    "winston": "^3.8.2",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.3.0",
    "@faker-js/faker": "^10.0.0",
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@types/chai": "^5.0.0",
    "@types/express": "^5.0.0",
    "@types/mocha": "^10.0.0",
    "@types/node": "^22.0.0",
    "@types/sinon": "^17.0.0",
    "@types/supertest": "^6.0.0",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/yamljs": "^0.2.31",
    "artillery": "^2.0.0-23",
    "chai": "^6.0.0",
    "eslint": "9.x",
    "forever": "^4.0.3",
    "globals": "^16.0.0",
    "husky": "^9.0.0",
    "mocha": "^11.0.0",
    "nodemon": "^3.0.0",
    "npm-run-all2": "^8.0.0",
    "nyc": "^17.0.0",
    "prettier": "^3.3.3",
    "rimraf": "^6.0.0",
    "sequelize-cli": "^6.5.1",
    "sinon": "^21.0.0",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.0",
    "typescript-eslint": "^8.0.0"
  },
  "packageManager": "yarn@1.22.22+sha256.c17d3797fb9a9115bf375e31bfd30058cac6bc9c3b8807a3d8cb2094794b51ca"
}
