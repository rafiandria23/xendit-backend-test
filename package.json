{
  "name": "@rafiandria23/xendit-backend-test",
  "version": "1.0.0",
  "packageManager": "yarn@4.9.4",
  "license": "MIT",
  "private": true,
  "main": "dist/index.js",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "test": "cross-env NODE_ENV=test mocha -p -r reflect-metadata -r ts-node/register src/**/*.test.ts -b",
    "test:load": "node scripts/load-test.js",
    "coverage": "nyc yarn test",
    "artillery": "artillery run src/tests/load/test.yml",
    "api": "node dist/index.js",
    "start": "cross-env NODE_ENV=production run-s clean compile api",
    "clean": "rimraf dist",
    "compile": "tsc",
    "compile:watch": "tsc --watch",
    "watch": "nodemon -q dist/index.js",
    "dev": "cross-env NODE_ENV=development run-p clean compile:watch watch",
    "lint": "eslint .",
    "pretty": "prettier . --write",
    "format": "run-s lint pretty",
    "prepare": "husky"
  },
  "dependencies": {
    "@hapi/boom": "^10.0.1",
    "cross-env": "^10.0.0",
    "express": "^5.1.0",
    "inversify": "^7.9.1",
    "joi": "^18.0.1",
    "reflect-metadata": "^0.2.2",
    "sequelize": "^6.37.7",
    "sequelize-typescript": "^2.1.6",
    "sqlite3": "^5.1.7",
    "swagger-ui-express": "^5.0.1",
    "winston": "^3.17.0",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.34.0",
    "@faker-js/faker": "^10.0.0",
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@types/chai": "^5.2.2",
    "@types/express": "^5",
    "@types/mocha": "^10.0.10",
    "@types/node": "^24.3.0",
    "@types/sinon": "^17.0.4",
    "@types/source-map-support": "^0.5.10",
    "@types/supertest": "^6.0.3",
    "@types/yamljs": "^0",
    "artillery": "^2.0.24",
    "chai": "^6.0.1",
    "eslint": "^9.34.0",
    "forever": "^4.0.3",
    "globals": "^16.3.0",
    "husky": "^9.1.7",
    "mocha": "^11.7.1",
    "nodemon": "^3.1.10",
    "npm-run-all2": "^8.0.4",
    "nyc": "^17.1.0",
    "prettier": "^3.6.2",
    "rimraf": "^6.0.1",
    "sequelize-cli": "^6.6.3",
    "sinon": "^21.0.0",
    "source-map-support": "^0.5.21",
    "supertest": "^7.1.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.41.0"
  }
}
